sort=-time
layer=overlay
background-color=#30383ecc
width=300
height=150
border-size=2
border-color=#1cd1ab
text-color=#e8faed
border-radius=16
icons=1
max-icon-size=64
default-timeout=5000
ignore-timeout=1
font=Cousine Nerd Font 14

[urgency=low]
border-color=#877d81

[urgency=normal]
border-color=#1cd1ab

[urgency=high]
border-color=#c84d4f
default-timeout=0

[app-name="mpd-notification"]
default-timeout=2000
group-by=category
on-notify=none

[app-name="discord"]
default-timeout=0
