sort=-time
layer=overlay
background-color=#2e3440
width=300
height=110
border-size=2
border-color=#88c0d0
border-radius=16
icons=0
max-icon-size=64
default-timeout=5000
ignore-timeout=1
font=Cousine Nerd Font 14
on-notify=exec tput bel

[urgency=low]
